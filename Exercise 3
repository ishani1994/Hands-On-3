cus.id1 <- runif(10, min=0, max=10)
View(cus.d1)
cus.id<- as.integer(cus.id1)
income.df <- rnorm(10, mean=250000, sd=75000)
View(income.df)
income_int<- as.integer(income.df)
gen_temp<- c(1:10)
gen_temp1<- sample(gen_temp)
gender <- ifelse(gen_temp<=60,"M","F")
cus.df<- data.frame(cus.id,income_int,gender)
View(cus.df)
